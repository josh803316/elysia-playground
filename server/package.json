{
  "name": "@josh803316/elysia-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "tsc --noEmit",
    "db:generate": "drizzle-kit generate --schema=./src/db/schema.ts --dialect=postgresql",
    "db:studio": "drizzle-kit studio --dialect=postgresql",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:seed": "bun run src/db/seed.ts"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.10",
    "@electric-sql/pglite": "^0.3.15",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.8",
    "@elysiajs/html": "^1.4.0",
    "@elysiajs/opentelemetry": "^1.4.10",
    "@elysiajs/swagger": "^1.3.1",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.24",
    "@elysiajs/static": "^1.4.7",
    "elysia-clerk": "^0.13.2",
    "jsonwebtoken": "^9.0.3",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.9"
  },
  "module": "src/index.js"
}
